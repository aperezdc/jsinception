#include "css.in"
#include "fonts.css.in"

.fallback-message {
	display: none;
}

@page land {
	size: landscape;
}

.step {
	border: 0;
	opacity: 1 !important;
	page-break-inside: avoid;
	page-break-after: always;
	position: relative !important;
	transform (none !important);
}

.step img {
	max-height: 100%;
}

body {
	height: auto !important;
	overflow: visible !important;
}

$impress {
	position: relative !important;
	top: 0 !important;
	left: 0 !important;
	transform_origin (none !important);
	transform (none !important);
}
